        "meta.ecmp_select": 0
      },
      "action_name": "MyIngress.set_nhop",
      "action_params": {
        "nhop_dmac": "00:00:00:00:04:02",
        "nhop_ipv4": "10.0.4.2",
        "port" : 1
      }
    },
    {
      "table": "MyIngress.pp_nport",
      "match": {
        "meta.ecmp_select": 1
      },
      "action_name": "MyIngress.set_nhop",
      "action_params": {
        "nhop_dmac": "00:00:00:00:04:02",
        "nhop_ipv4": "10.0.4.2",
        "port" : 1
      }
    },
    {
      "table": "MyEgress.send_frame",
      "match": {
        "standard_metadata.egress_port": 1
      },
      "action_name": "MyEgress.rewrite_mac",
      "action_params": {
        "smac": "00:00:00:04:02:00"
      }
    }
  ]
